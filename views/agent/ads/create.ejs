	$("#submit").click(function () {
		let check = [];
		let errors = [];
		$('input,textarea').filter('[required]:visible').each(function () {
			if (!$(this).val().length) {
				check.push('e');
				errors.push($(this).parent('div').children('label').html());
			}
		});

		if (check.length == 0)
			$("#myForm").submit();
		else {
			let strError = "";
			errors.map(error => {
				strError += `● ${error} را وارد کنید </br>`;
			})
			$(".alert-danger").html(strError);
			$(".alert-danger").show();
