<style>
	input:required:invalid {
		background-color: #fceaea;
	}

	input:required:valid {
		background-color: #effaef;
	}

	input[type="radio"]:required:invalid,
	input[type="radio"]:required:valid {
		background-color: var(--dark-gray);
	}

	input[type="radio"]:required:valid:checked {
		background-color: var(--accent);
	}

	textarea:required:invalid {
		background-color: #fceaea;
	}

	textarea:required:valid {
		background-color: #effaef;
	}

	.material {
		background-color: var(--light-gray);
		border-radius: 3px;
		margin: 0px 10px;
		text-align: center;
		padding: 5px;
		width: 50px;
	}

	.input-checkbox {
		width: 20px;
		height: 20px;
		cursor: pointer;
	}

	.label-checkbox {
		cursor: pointer;
		margin: 5px 10px 5px 0px;
	}

	.checkbox-body {
		display: flex;
		flex-wrap: wrap;
		padding-left: 0;
	}

	.checkbox-item {
		display: flex;
		flex-direction: row;
		justify-content: center;
		align-items: center;
		margin: 5px;
		padding: 10px;
	}

	.button-prev,
	.button-next {
		background-color: var(--main);
		color: var(--white);
		float: left;
		margin: 15px 0px 0px 10px;
	}

	.button-prev:hover,
	.button-next:hover {
		background-color: var(--primary);
		color: var(--white);
	}

	.swiper-pagination-bullet {
		width: 25px;
		height: 25px;
		text-align: center;
		line-height: 27px;
		font-size: 12px;
		font-weight: bold;
		color: #000;
		opacity: 1;
		background: rgba(0, 0, 0, 0.2);
		margin: 10px 4px !important;
	}

	.swiper-pagination-bullet-active {
		color: #fff;
		background: var(--accent);
	}

	.swiper-button-disabled:hover,
	.swiper-button-disabled {
		background-color: var(--middle-gray);
	}

	.toggle,
	.toggle::after,
	.toggle *,
	.toggle *::after {
		transition: 0.25s ease all;
	}

	.toggle {
		display: inline-block;
		position: relative;
		width: 60px;
		height: 34px;
		margin: 0.5rem;
	}

	.toggle input {
		display: none;
	}

	.toggle .trigger {
		position: absolute;
		top: 0;
		left: 0;
		right: 0;
		bottom: 0;
		background: #666;
	}

	.toggle .trigger,
	.toggle .trigger::after {
		border-radius: 1rem;
	}

	.toggle .trigger::after {
		position: absolute;
		content: "";
		height: 26px;
		width: 26px;
		left: 4px;
		bottom: 4px;
		background-color: white;
	}

	.toggle .trigger:hover {
		cursor: pointer;
	}

	.toggle input:checked~.trigger {
		background: var(--accent);
	}

	.toggle input:checked~.trigger::after {
		transform: translateX(100%);
	}

	.toggle.x2 {
		transform: scale(2);
	}

	.map {
		min-height: 350px;
		width: 100%;
		background: #eee;
		border: 2px solid #aaa;
		margin-bottom: 15px;
	}
</style>

